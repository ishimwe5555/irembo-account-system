{
  "name": "irembo-account-managment",
  "version": "1.0.0",
  "description": "Irembo account managment",
  "main": "index.js",
  "scripts": {
    "start": "babel-node build/index.js",
    "lint": "eslint \"src/**/*.{js,css,html,json,md}\"",
    "prettier": "eslint \"src/**/*.{js,css,html,json,md}\"",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint --fix --ext .js .",
    "prettier:fix": "prettier --write \"src/**/*.{js,css,html,json,md}\"",
    "prettier:check": "prettier \"src/**/*.{js,css,html,json,md}\"",
    "prepare": "husky install",
    "test": "mocha --require @babel/register src/tests/ --recursive --exit --timeout 80000",
    "coverage": "nyc npm run test",
    "build": "npx babel src --out-dir build",
    "prestart": "npm run build",
    "make_model": "npx sequelize-cli model:generate --name",
    "make_migration": "npx sequelize-cli migration:create --name",
    "migrate": " npx sequelize-cli db:migrate",
    "migrate_undo": " npx sequelize-cli db:migrate:undo",
    "migrate_undo_all": "npx sequelize-cli db:migrate:undo:all",
    "make_seed": " npx sequelize-cli seed:generate --name",
    "seed": " npx sequelize-cli db:seed:all",
    "seed_undo": " npx sequelize-cli db:seed:undo",
    "seed_undo_all": " npx sequelize-cli db:seed:undo:all"
  },
  "author": "team-sostene",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/Team-Sostene-E-commerce-bn/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.5",
    "bcryptjs": "^2.4.3",
    "c8": "^7.13.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "mocha": "^10.2.0",
    "morgan": "^1.10.0",
    "nyc": "^15.1.0",
    "passport-stub": "^1.1.1",
    "prettier": "^2.8.4",
    "sequelize-cli": "^6.6.0"
  },
  "dependencies": {
    "@babel/node": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.21.0",
    "@sendgrid/mail": "^7.7.0",
    "babel-cli": "^6.26.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
