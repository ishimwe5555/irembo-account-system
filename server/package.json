    "cloudinary-build-url": "^0.2.4",
    "connect-redis": "^7.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint-plugin-node": "^11.1.0",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.4.0",
    "express-session": "^1.17.3",
    "faker": "^6.6.6",
